          {
            "extType": "pc_anchor_panel",
            "platform": "pc",
            "entry": "index_streamer.js"
          }
        ]
      },
      "useTaf": false,
      "https": true,
      "host": "172.21.108.22",
      "useCssLoader": true,
      "port": 18080,
      "enableLint": {
        "stylelint": false,
        "eslint": false
